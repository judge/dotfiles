snippet php
  <?php ${1:TARGET} ?>

snippet e
  echo ${1:TARGET};

snippet pr
  echo "<pre>"; print_r(${1:TARGET}); echo "</pre>";

snippet foreach
  foreach( ${1} as ${2} ) {
    ${3}
  }

snippet function
abbr public function () {}
  ${1:public }function ${2:#:FunctionName}(${3}) {
    ${4:TARGET}
  }
